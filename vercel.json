{
  "rewrites": [
    {
      "source": "/",
      "destination": "/templates/login.html"
    },
    {
      "source": "/login",
      "destination": "/templates/login.html"
    },
    {
      "source": "/register",
      "destination": "/templates/register.html"
    },
    {
      "source": "/dashboard",
      "destination": "/templates/dashboard.html"
    }
  ],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
} 